{
  "plugins": ["transform-runtime"],
  "presets": [
    ["es2015", { "modules": false }],
    "react",
    "stage-0"
  ],
  "env": {
    "test": {
      "presets": ["es2015", "react", "stage-0"]
    },
    "production": {
      "plugins": ["transform-runtime", "transform-react-remove-prop-types"]
    }
  }
}
